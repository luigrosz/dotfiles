{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_assistant": false
  },
  "theme": "One Dark",
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "soft_wrap": "editor_width",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "format_on_save": "on",
  "tab_size": 2,
  "formatter": "language_server",
  "auto_install_extensions": {
    "html": true,
    "dockerfile": true,
    "docker-compose": true,
    "csv": true,
    "git-firefly": true,
    "make": true,
    "rainbow-csv": true,
    "sql": true,
    "svelte": true,
    "toml": true,
    "xml": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}

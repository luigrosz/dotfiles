{
  "agent": {
    "default_model": {
      "provider": "google",
      "model": "gemini-3-flash-preview"
    },
    "default_profile": "write",
    "model_parameters": []
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_assistant": false
  },
  "theme": "One Dark",
  "assistant": {
    "default_profile": "write",
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "version": "2"
  },
  "soft_wrap": "editor_width",
  "buffer_font_family": "FiraMono Nerd Font Mono",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "format_on_save": "on",
  "tab_size": 2,
  "formatter": "language_server",
  "auto_install_extensions": {
    "html": true,
    "dockerfile": true,
    "docker-compose": true,
    "csv": true,
    "git-firefly": true,
    "make": true,
    "rainbow-csv": true,
    "sql": true,
    "svelte": true,
    "toml": true,
    "xml": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
